    echo $pass |myproxyclient logon -b -T -S -s $server -l $username -o $ESG_CREDENTIALS -C $ESG_CERT_DIR

## -b: bootstrap trus roots, don't verify server's SSL certificate against CA certificates
## -T: updateTrustRoots
## -S: password from stdin
## -s:
## -l: username
##-o: file to strore credentials. Now: == $HOME/.esg/credentials.pem -- change that to inside esget directory
## -C: location of trusted certificates

$PKI_WGET_OPTS --certificate=$ESG_CERT --private-key=$ESG_KEY --save-cookies=$COOKIE_JAR --load-cookies=$COOKIE_JAR"

wget is run with options $PKI_WGET_OPTS

